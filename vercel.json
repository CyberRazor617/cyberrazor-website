{
  "name": "cyberrazor-website",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@cyberrazor-api-url",
    "NEXT_PUBLIC_WS_URL": "@cyberrazor-ws-url"
  },
  "functions": {
    "app/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
